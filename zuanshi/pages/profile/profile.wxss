/* pages/profile/profile.wxss */
.avatar{
  background: #fff;
  height: 400rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.avatar image{
  width: 100%;
  height: 350rpx;
  border-radius: 5px;
}
.divider5{
  position: fixed;
  bottom: 0px;
  height: 100rpx;
  background: #fff;
  align-items: center;
  display: flex;
  justify-content: space-between;
  font-size: 33rpx;
  color: #000;
  width: 100%;
  border-top: 1rpx solid  #eee;
}
.item1{
  width: 33.333%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  font-size: 20rpx;
}
.item1 image{
  width: 25px;
  height: 25px;
}
